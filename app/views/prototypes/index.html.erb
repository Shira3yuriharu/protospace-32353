<main class="main">
  <div class="inner">
    <%# ログインしているときは以下を表示する %>
      <div class="greeting">
        <% if user_signed_in? %>
          <h4> こんにちは、<%= link_to current_user.name, user_path(current_user), class: :greeting__link, method: :get %><a class="greeting__link">さん</a></h4>
        <% end %>
      </div>
    <%# // ログインしているときは上記を表示する %>
    <div class="card__wrapper">
      <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
      <%= render partial: 'prototype', collection: @prototypes %>
    </div>
  </div>
</main>
